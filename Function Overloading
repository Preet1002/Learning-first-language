#include<iostream>
using namespace std;
int volume(double r,int h){
return (3.14*r*r*h) ;
}
int volume (int a) {
return a*a*a;
}
int volume (int l, int b, int h) {
return l*b*h;
}
int main(){
cout<<"The volume of cube with dimension 5 is "<<volume(5)<<endl;
cout<<"The volume of cylinder with dimension 5,6 is "<<volume(5,6)<<endl;
cout<<"The volume of cuboid with dimension 5,6,7 is "<<volume(5,6,7)<<endl;
return 0;
}
